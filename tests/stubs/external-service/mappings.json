{
    "mappings": [
        {
            "request": {
                "method": "GET",
                "url": "/my-account"
            },
            "response": {
                "status": 401
            }
 
        },
        {
            "request": {
                "method": "GET",
                "url": "/my-account",
                "headers": {
                    "Authorization": {
                        "equalTo": "Bearer a.valid.token"
                    }
                }
            },
            "response": {
                "status": 200,
                "jsonBody": {
                    "firstName": "John",
                    "lastName": "Doe"
                }
            }
        }
    ]
}